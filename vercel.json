{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/receive",
      "dest": "/api/receive.js"
    },
    {
      "src": "/api/command",
      "dest": "/api/command.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.js"
    },
    {
      "src": "/api/list",
      "dest": "/api/list.js"
    },
    {
      "src": "/receive",
      "dest": "/api/receive.js"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
